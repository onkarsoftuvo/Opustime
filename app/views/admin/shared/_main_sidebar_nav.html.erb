<aside class="main-sidebar">
  <!-- sidebar: style can be found in sidebar.less -->
  <section class="sidebar">
    <!-- Sidebar user panel -->
    <div class="user-panel">
      <div class="pull-left image ">
        <%= image_tag current_owner.try(:logo).present? ? current_owner.logo.url(:thumb) : 'default_admin', {class: "user-image"} %>
      </div>
      <div class="pull-left info">
        <p><%= current_owner.full_name rescue '' %></p>
        <a href="#"><i class="fa fa-circle text-success"></i> Online</a>
      </div>
    </div>
    <!-- search form -->
    <ul class="sidebar-menu">
      <li class="header">MAIN NAVIGATION</li>
      <li>
        <%= link_to home_admin_dashboards_path, class: "treeview active" do %>
            <i class="fa fa-dashboard "></i>
            <span>Dashboard</span>
        <% end %>
      </li>
      <% unless @admin_user_permission.nil?  %>
          <% if @admin_user_permission.business_report.values.map.include?(true) %>
              <li class="treeview">
                <a href="#">
                  <i class="fa fa-files-o"></i>
                  <span>Business Reports</span>
                </a>
                <ul class="treeview-menu">
                  <% if @admin_user_permission.business_report['list'] %>
                  <li class=current_class?(list_business_list_path)>
                    <%= link_to list_business_list_path, :class => current_class?(list_business_list_path) do %>
                        <i class="fa fa-circle-o"></i>
                        <span>Business List Report</span>
                    <% end %>
                  </li>
                  <% end %>
                  <% if @admin_user_permission.business_report['customer'] %>
                  <li>
                    <%= link_to list_business_customer_path do %>
                        <i class="fa fa-circle-o"></i>
                        <span>Business Customer Report </span>
                    <% end %>
                  </li>
                  <% end %>
                  <% if @admin_user_permission.business_report['earnbs'] %>
                  <li>
                    <%= link_to earn_business_earning_path do %>
                        <i class="fa fa-circle-o"></i>
                        <span>Top Earning Business</span>
                    <% end %>
                  </li>
                  <% end %>
                  <% if @admin_user_permission.business_report['earnloc'] %>
                  <li>
                    <%= link_to earn_business_location_path do %>
                        <i class="fa fa-circle-o"></i>
                        <span>Top Earning Location</span>
                    <% end %>
                  </li>
                  <% end %>
                  <% if @admin_user_permission.business_report['financial'] %>
                  <li>
                    <%= link_to earn_business_financial_path do %>
                        <i class="fa fa-circle-o"></i>
                        <span>Business Financial Report</span>
                    <% end %>
                  </li>
                  <% end %>
                </ul>
              </li>
          <% end %>
      <% else  %>
          <li class="treeview">
            <a href="#">
              <i class="fa fa-files-o"></i>
              <span>Business Reports</span>
            </a>
            <ul class="treeview-menu">
              <li class=current_class?(list_business_list_path)>
                <%= link_to list_business_list_path, :class => current_class?(list_business_list_path) do %>
                    <i class="fa fa-circle-o"></i>
                    <span>Business List Report</span>
                <% end %>
              </li>
              <li>
                <%= link_to list_business_customer_path do %>
                    <i class="fa fa-circle-o"></i>
                    <span>Business Customer Report </span>
                <% end %>
              </li>
              <li>
                <%= link_to earn_business_earning_path do %>
                    <i class="fa fa-circle-o"></i>
                    <span>Top Earning Business</span>
                <% end %>
              </li>
              <li>
                <%= link_to earn_business_location_path do %>
                    <i class="fa fa-circle-o"></i>
                    <span>Top Earning Location</span>
                <% end %>
              </li>
              <li>
                <%= link_to earn_business_financial_path do %>
                    <i class="fa fa-circle-o"></i>
                    <span>Business Financial Report</span>
                <% end %>
              </li>
            </ul>
          </li>
      <% end %>
      <% unless @admin_user_permission.nil?  %>
          <% if @admin_user_permission.financial_report.values.map.include?(true) %>
              <li class="treeview">
                <a href="#">
                  <i class="fa fa-credit-card" aria-hidden="true"></i>
                  <span>Financial Report</span>
                </a>
                <ul class="treeview-menu">
                  <% if @admin_user_permission.financial_report['list'] %>
                  <li>
                    <%= link_to list_financial_list_path do %>
                        <i class="fa fa-circle-o"></i>
                        <span>Business List Report</span>
                    <% end %>
                  </li>
                  <% end %>
                  <% if @admin_user_permission.financial_report['earn'] %>
                  <li>
                    <%= link_to earn_financial_earn_path do %>
                        <i class="fa fa-circle-o"></i>
                        <span>Top Earning Business</span>
                    <% end %>
                  </li>
                  <% end %>
                </ul>
              </li>
          <% end %>
      <% else %>
          <li class="treeview">
            <a href="#">
              <i class="fa fa-money" aria-hidden="true"></i>
              <span>Financial Report</span>
            </a>
            <ul class="treeview-menu">
              <li>
                <%= link_to list_financial_list_path do %>
                    <i class="fa fa-circle-o"></i>
                    <span>Business List Report</span>
                <% end %>
              </li>
              <li>
                <%= link_to earn_financial_earn_path do %>
                    <i class="fa fa-circle-o"></i>
                    <span>Top Earning Business</span>
                <% end %>
              </li>
            </ul>
          </li>
      <% end %>





      <% unless @admin_user_permission.nil?  %>
          <% if @admin_user_permission.trial_user %>
          <li>
            <%= link_to list_trial_user_path, class: "treeview" do %>
                <i class="fa fa-clock-o" aria-hidden="true"></i>
                <span>Trial User</span>
            <% end %>
          </li>
          <% end %>
      <% else %>
          <li>
            <%= link_to list_trial_user_path, class: "treeview" do %>
                <i class="fa fa-clock-o" aria-hidden="true"></i>
                <span>Trial User</span>
            <% end %>
          </li>
      <% end %>
      <% unless @admin_user_permission.nil?  %>
          <% if @admin_user_permission.subscription.values.map.include?(true) %>
              <li class="treeview">
                <a href="#">
                  <i class="fa fa-industry" aria-hidden="true"></i>
                  <span>Subscription</span>
                </a>
                <ul class="treeview-menu">
                  <% if @admin_user_permission.subscription['list'] %>
                  <li>
                    <%= link_to admin_subscription_index_path do %>
                        <i class="fa fa-circle-o"></i>
                        <span>Subscription Package</span>
                    <% end %>
                  </li>
                  <% end %>
                  <% if @admin_user_permission.subscription['add'] %>
                  <li>
                    <%= link_to new_admin_subscription_path do %>
                        <i class="fa fa-circle-o"></i>
                        <span>Add Subscription Package</span>
                    <% end %>
                  </li>
                  <% end %>
                </ul>
              </li>
          <% end %>
      <% else %>
          <li class="treeview">
            <a href="#">
              <i class="fa fa-credit-card" aria-hidden="true"></i>
              <span>Subscription</span>
            </a>
            <ul class="treeview-menu">
              <li>
                <%= link_to admin_subscription_index_path do %>
                    <i class="fa fa-circle-o"></i>
                    <span>Subscription Package</span>
                <% end %>
