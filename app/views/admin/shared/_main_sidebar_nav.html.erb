                <% end %>
              </li>
              <li>
                <%= link_to others_business_subscription_path do %>
                    <i class="fa fa-circle-o"></i>
                    <span>Business Subscription</span>
                <% end %>
              </li>
              <li>
                <%= link_to others_business_payments_path do %>
                    <i class="fa fa-circle-o"></i>
                    <span>Business Payments</span>
                <% end %>
              </li>
            </ul>
          </li>
      <% end %>


      <% unless @admin_user_permission.nil?  %>
          <% if @admin_user_permission.permission  %>
          <li class="treeview">
            <%= link_to admin_permission_path do %>
                <i class="fa fa-key" aria-hidden="true"></i>
                <span>Permission</span>
            <% end %>
          </li>
          <% end %>
      <% else %>
          <li class="treeview">
            <%= link_to admin_permission_path do %>
                <i class="fa fa-key" aria-hidden="true"></i>
                <span>Permission</span>
            <% end %>
          </li>
      <% end %>
      <% unless @admin_user_permission.nil?  %>
          <% if @admin_user_permission.logs  %>
              <li class="treeview">
                <%= link_to admin_logs_opustime_logs_path do %>
                    <i class="fa fa-bug" aria-hidden="true"></i>
                    <span>Logs</span>
                <% end %>
              </li>
          <% end %>
      <% else %>
          <li class="treeview">
            <%= link_to admin_logs_opustime_logs_path do %>
                <i class="fa fa-bug" aria-hidden="true"></i>
                <span>Logs</span>
            <% end %>
          </li>
      <% end %>


      <% if current_owner.try(:role).eql?('super_admin_user') %>
          <li class="treeview">
            <%= link_to admin_user_permission_index_path do %>
                <i class="fa fa-key" aria-hidden="true"></i>
                <span>Admin's users Permissions </span>
            <% end %>
          </li>

          <li>
            <%= link_to admin_approvals_path do %>
                <i class="fa fa-check" aria-hidden="true"></i>
                <span>Approvals</span>
            <% end %></li>
          </li>
      <% end %>

    </ul>
  </section>
  <!-- /.sidebar -->
</aside>
