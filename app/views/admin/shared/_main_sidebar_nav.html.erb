                    <% end %>
                  </li>
                  <% end %>
                  <% if @admin_user_permission.sms['consm'] %>
                  <li>
                    <%= link_to sms_consumption_path do %>
                        <i class="fa fa-circle-o"></i>
                        <span>SMS Consumption</span>
                    <% end %>
                  </li>
                  <% end %>
                </ul>
              </li>
          <% end %>
      <% else %>
          <li class="treeview">
            <a href="#">
              <i class="fa fa-comments-o" aria-hidden="true"></i>
              <span>SMS</span>
            </a>
            <ul class="treeview-menu">
              <li>
                <%= link_to admin_sms_groups_path do %>
                    <i class="fa fa-circle-o"></i>
                    <span>SMS Groups</span>
                <% end %>
              </li>
              <li>
                <%= link_to admin_sms_path do %>
                    <i class="fa fa-circle-o"></i>
                    <span>SMS Package</span>
                <% end %>
              </li>
              <li>
                <%= link_to sms_consumption_path do %>
                    <i class="fa fa-circle-o"></i>
                    <span>SMS Consumption</span>
                <% end %>
              </li>
              <li>
                <%= link_to sms_number_path do %>
                    <i class="fa fa-circle-o"></i>
                    <span>SMS Number</span>
                <% end %>
              </li>
            </ul>
          </li>
      <% end %>

      <% unless @admin_user_permission.nil?  %>
          <% if @admin_user_permission.others.values.map.include?(true) %>
              <li class="treeview">
                <a href="#">
                  <i class="fa fa-eraser" aria-hidden="true"></i>
                  <span>Others</span>
                </a>
                <ul class="treeview-menu">
                  <% if @admin_user_permission.others['delete'] %>
                  <li>
                    <%= link_to others_request_path do %>
                        <i class="fa fa-circle-o"></i>
                        <span>Import Delete Request</span>
                    <% end %>
                  </li>
                  <% end %>
                  <% if @admin_user_permission.others['bs'] %>
                  <li>
                    <%= link_to others_business_subscription_path do %>
                        <i class="fa fa-circle-o"></i>
                        <span>Business Subscription</span>
                    <% end %>
                  </li>
                  <% end %>
                  <% if @admin_user_permission.others['payment'] %>
                  <li>
                    <%= link_to others_business_payments_path do %>
                        <i class="fa fa-circle-o"></i>
                        <span>Business Payments</span>
                    <% end %>
                  </li>
                  <% end %>
                </ul>
              </li>
          <% end %>
      <% else %>
          <li class="treeview">
            <a href="#">
              <i class="fa fa-eraser" aria-hidden="true"></i>
              <span>Others</span>
            </a>
            <ul class="treeview-menu">
              <li>
                <%= link_to others_request_path do %>
                    <i class="fa fa-circle-o"></i>
