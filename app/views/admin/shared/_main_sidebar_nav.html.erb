              <li>
                <%= link_to new_admin_subscription_path do %>
                    <i class="fa fa-circle-o"></i>
                    <span>Add Subscription Package</span>
                <% end %>
              </li>
            </ul>
          </li>

      <% end %>

      <% unless @admin_user_permission.nil?  %>
          <% if @admin_user_permission.sms.values.map.include?(true) %>
