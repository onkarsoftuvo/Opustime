<div class="tab-pane" id="tab_9">
                          <div class="row">
                            <div class="col-md-12">
                              <div class="form-group filter-con">
                                <label>Filters</label>
                                <!-- <div class="col-md-3">
                                  <select class="form-control">
                                    <option>Product Name</option>
                                  </select>
                                </div> -->
                                <div class="col-md-3">
                                <%= form_tag("/business/#{@comp.id}/details_list#tab_9", :method => :get)  do %>
                                  <%= text_field_tag :product_name, params[:product_name], :placeholder => 'Product Name', class: :"form-control"%>
                                  <%= hidden_field_tag :tab_no , 8 %>
                                </div>
                                <div class="col-md-1">
                                  <%= submit_tag "Filter", class: "btn btn-large btn-primary btns" %>
                                </div>
                                <div class="col-md-2 pull-right text-right">
                                  <!-- <button class="btn btn-default"> -->
                                  <%= link_to "business/#{@comp.id}/details_list#tab_9", class: "btn btn-default" do %>Clear Filter
                                  <%end%>
                                  <!-- </button> -->
                                </div>
                              </div>
                              <%end%>
                            </div>
                            <div class="col-md-12">
                              <table id="example10" class="table table-bordered custom_table">
                                <thead>
                                <tr>
                                  <!--<th class="no_sort text-center select_all"></th>-->
                                  <th>Sr No</th>
                                  <th>Product name</th>
                                  <th>Stock</th>
                                  <th>Added Date</th>
                                  <th>Stock Updated</th>
                                  <th>Stock Log for Product </th>
                                  <!-- <th class="no_sort text-center">Actions</th> -->
                                </tr>
                                </thead>
                                <tbody>
                                
                                <% @products.each do |product|%>
                                <tr>
                                  <!--<td class="text-center">-->
                                    <!--<input type="checkbox" name="" class="minimal">-->
                                  <!--</td>-->
                                  <td><%= @count_product += 1%></td>
                                 
                                  <td><%= product.name%></td>
                                  <td><%= product.try(:stock_number)%></td>
                                  <td><%= product.created_at.try(:strftime, '%B %e, %Y')%></td>
                                  <td><%= product.updated_at.try(:strftime, '%B %e, %Y')%></td>
                                  <td class="text-center"><%= product.status%></td>
                                  <!-- <td class="action_btns text-center">
                                    <div class="btn-group">
                                      <button type="button" class="btn btn-default"><i class="fa fa-pencil-square-o"></i></button>
                                      <button type="button" class="btn btn-default"><i class="fa fa-eye"></i></button>
                                      <button type="button" class="btn btn-default"><i class="fa fa-credit-card"></i></button>
                                    </div>
                                  </td> -->
                                </tr>
                                <% end %>
                                </tbody>
                              </table>
                            </div>
                          </div>
                          <div class="clearfix"></div>
                        </div>