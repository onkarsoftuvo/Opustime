<div class="tab-pane" id="tab_4">
                          <div class="row">
                            <div class="col-md-12">
                              <div class="form-group filter-con">
                                <label>Filters</label>
                                <div class="col-md-3">
                                <%= form_tag("/business/#{@comp.id}/details_list#tab_4", :method => :get)  do %>
                                  <%= text_field_tag :Subscription, params[:Subscription], :placeholder => 'Subscription Name', class: :"form-control"%>
                                  <%= hidden_field_tag :tab_no , 3 %>
                                </div>
                                <div class="col-md-3">
                                  <select id="states-of-country" name="project[country]" class="form-control">
                                  <option id="no-state" value="" selected>Select Your Country</option>
                              
                                  /* For states within IN */
                                  <% customise_state(CS.countries).each do |arr| %>
                                      <option value="<%= arr.last %>" id="<%= arr.last %>"><%= arr.first %></option>
                                    <% end %>
                                  </select>
                                </div>
                                <div class="col-md-1">
                                  <button class="btn btn-info">Filter</button>
                                </div>
                               <!-- <div class="col-md-2 pull-right text-right">
                                  <button class="btn btn-default">Clear Filter</button>
                                </div> -->
                                 <%= link_to "business/#{@comp.id}/details_list#tab_4", class: "btn btn-default" do %>Clear Filter
                                  <%end%>
                                <%end%>
                              </div>
                            </div>
                            <div class="box-header custom_heading">
                              <h3 class="box-title">Paid Subscription</h3>
                            </div>
                            <div class="col-md-12">
                              <table id="example4" class="table table-bordered custom_table">
                                <thead>
                                <tr>
                                  <!--<th class="no_sort text-center select_all"></th>-->
                                  <th>Plan Name</th>
                                  <th>Date of Acitvation</th>
                                  <th>Amount</th>
                                  <th>Status</th>
                                  <th>Previous Plan</th>
                                </tr>
                                </thead>
                                <tbody>
                                <tr>
                                  <!--<td class="text-center">-->
                                    <!--<input type="checkbox" name="" class="minimal">-->
                                  <!--</td>-->
                                 
                                  <td><%= @comp.subscription.name %></td>
                                  <td><%= @comp.subscription.purchase_date %></td>
                                  <td>$ <%= @comp.subscription.cost %></td>
                                  <% if @comp.subscription.is_subscribed == false%>
                                    <td>Active(Trial)</td>
                                  <%else%>
                                    <td>Active</td>
                                  <%end%>
                                  <td>NA</td>
                                  <!-- <td class="action_btns text-center">
                                    <div class="btn-group">
                                      <button type="button" class="btn btn-default"><i class="fa fa-pencil-square-o"></i></button>
                                      <button type="button" class="btn btn-default"><i class="fa fa-eye"></i></button>
                                      <button type="button" class="btn btn-default"><i class="fa fa-credit-card"></i></button>
                                    </div>
                                  </td> -->
                                </tr>
                                </tbody>
                              </table>
                            </div>
                            <div class="col-md-12">
                              <hr>
                            </div>
                            <div class="box-header custom_heading">
                              <h3 class="box-title">Trail Subscription</h3>
                              <button type="button" class="btn btn-primary pull-right btn-sm" data-toggle="modal" data-target="#myModal">Add Trail Period</button>
                            </div>
                            <div class="col-md-12">
                              <table id="example5" class="table table-bordered custom_table">
                                <thead>
                                <tr>
                                  <!--<th class="no_sort text-center select_all"></th>-->
                                  <th>Start Date of Plans</th>
                                  <th>End Date</th>
                                  <th>Plan Type</th>
                                  <th class="no_sort text-center">Actions</th>
                                </tr>
                                </thead>
                                <tbody>
                                <tr>
                                  <!--<td class="text-center">-->
                                    <!--<input type="checkbox" name="" class="minimal">-->
                                  <!--</td>-->
                                  <td><%= @comp.subscription.purchase_date %></td>
                                  <td><%= @subscription_details[:next_billing_date]%></td>
                                  <td><%= @comp.subscription.name %></td>
                                  <td class="action_btns text-center">
                                    <div class="btn-group">
                                      <!-- <button type="button" class="btn btn-default"><i class="fa fa-pencil-square-o"></i></button>
                                      <button type="button" class="btn btn-default"><i class="fa fa-eye"></i></button> -->
                                      <button type="button" class="btn btn-default"><i class="fa fa-credit-card"></i></button>
                                    </div>
                                  </td>
                                </tr>
                                </tbody>
                              </table>
                            </div>
                          </div>
                          <div class="clearfix"></div>
                        </div>